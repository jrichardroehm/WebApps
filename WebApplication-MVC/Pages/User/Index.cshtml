@model List<WebApplication_MVC.Models.UserViewModel>
@using WebApplication_MVC.Controllers

<h2>Generate the data!</h2>
<table >
    <thead>
    <tr>
        <th>First Name</th><th>Last Name</th><th>CacEDIPI</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.CacEdipi</td>
        </tr>
    }
    </tbody>
</table>